{
    "name": "matthewnw/laravel-permissions",
    "description": "Permission handling for users in Laravel 5.5 and up",
    "keywords": [
        "codelaunch",
        "laravel",
        "permissions",
        "acl",
        "security"
    ],
    "homepage": "https://gitlab.codelaunch.uk/packages/laravel-permissions",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Williams",
            "email": "matthew@codelaunch.uk"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/auth": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/container": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/contracts": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/database": "~5.5.0|~5.6.0|~5.7.0|~5.8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8.0",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.3",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Matthewnw\\Permissions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Matthewnw\\Permissions\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matthewnw\\Permissions\\PermissionsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
